{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "time-wait"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "../node_modules/resource/jquery/jquery-2.1.0.js",
        "../node_modules/resource/bootstrap/js/bootstrap.js",
        "../node_modules/resource/pace/pace.js",
        "../node_modules/urijs/src/URI.js",
        "../node_modules/sweetalert/dist/sweetalert.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/resource/daterangepicker/daterangepicker.js",
        "../node_modules/resource/datepicker/bootstrap-datepicker.js",
        "../node_modules/resource/datepicker/locales/bootstrap-datepicker.zh-CN.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
